    "scripts": {
        "dev": "electron-vite dev",
        "web": "vite --config electron.vite.config-browser.mts",
        "can-i-use": "pnpx update-browserslist-db@latest",
        "....................................................................................1": ".......................................................",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:os-utils:watch": "pnpm test -- os-utils.test.ts --run",
        "test:os-utils": "tsx src/renderer/src/utils/os-utils.test-report.ts",
        "..................................................................................1.1": ".......................................................",
        "build": "pnpm run tsc:build:all && upen && electron-vite build",
        "preview": "electron-vite preview",
        "postinstall": "electron-builder install-app-deps",
        "....................................................................................2": ".......................................................",
        "build:win": "pnpm run build && electron-builder --win",
        "build:unpack": "pnpm run build && electron-builder --dir",
        "....................................................................................a": ".......................................................",
        "build:web": "vite --config electron.vite.config-browser.mts build --outDir ../../dist --emptyOutDir",
        "build:web:github": "pnpm run build:web && gh-pages -d ./dist",
        "build:web:preview": "vite --config electron.vite.config-browser.mts preview --outDir ../../dist",
        "....................................................................................3": ".......................................................",
        "tsc": "tsc -w --noEmit -p tsconfig.web.json --composite false",
        "tsc:all": "pnpm run tsc:node && pnpm run tsc:web && pnpm run tsc:vite && pnpm run tsc:i18n",
        "tsc:build:all": "pnpm run tsc:web && pnpm run tsc:node",
        "tsc:web": "tsc --noEmit -p tsconfig.web.json --composite false",
        "tsc:node": "tsc --noEmit -p tsconfig.node.json --composite false",
        "tsc:i18n": "tsc --noEmit -p tsconfig.node.i18n.json --composite false",
        "tsc:vite": "tsc --noEmit -p tsconfig.node.vite.json --composite false",
        "....................................................................................5": ".......................................................",
        "i18n:extract": "tsx scripts/extract-i18n/extract-i18n-strings.ts --exclude-paths src/renderer/src/ui/icons",
        "i18n:ast": "tsx scripts/i18n-ast/0-main.ts --config scripts/config-i18n-ast.json5"
