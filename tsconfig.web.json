{
    "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
    "include": [
        "./src/renderer/src/vite-typescript/env.d.ts",
        "./src/renderer/src/**/*",
        "./src/renderer/src/**/*.tsx",
        "./src/electron/preload/*.d.ts",
        "./src/electron/app/**/*",
        "./src/shared/**/*"
    ],
    "compilerOptions": {
        "composite": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noImplicitReturns": false,
        "jsx": "react-jsx",
        "paths": {
            "@/*": [
                "./src/renderer/src/*"
            ],
            "@ui/*": [
                "./src/renderer/src/ui/*"
            ],
            "@renderer/*": [
                "./src/renderer/src/*"
            ],
            "@electron/*": [
                "./src/electron/app/*"
            ],
            "@shared/*": [
                "./src/shared/*"
            ]
        }
    }
}